{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["standard", "jsx-a11y", "prettier"],
  "env": {
    "es6": true
  },
  "rules": {
    "no-console": ["error", { "allow": ["info", "warn", "error"] }],
    "prettier/prettier": "error",
    "jsx-a11y/click-events-have-key-events": "off"
  }
}
