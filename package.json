{
  "name": "carbulator",
  "title": "Carbulator",
  "description": "A carbohydrate and insulin therapy calculator",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "webpack-dev-server --mode=development --hot",
    "build": "npm i && webpack --mode=production",
    "build:dev": "npm i && webpack --mode=development",
    "test": "node_modules/.bin/eslint . -c ./.eslintrc",
    "fix": "node_modules/.bin/eslint . --fix",
    "v:pre": "branch=`git rev-parse --abbrev-ref HEAD`; prerelease=\"npm version prerelease --preid ${branch//[\\/]/-}\"; eval $prerelease;",
    "v:patch": "npm version patch",
    "v:minor": "npm version minor",
    "v:major": "npm version major"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "css-loader": "^2.1.1",
    "framework7": "^4.4.3",
    "framework7-react": "^4.4.3",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "styled-components": "^5.0.0-beta.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "devDependencies": {
    "@wolfiezero/eslint-config": "^0.0.7",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "redux-devtools-extension": "^2.13.8",
    "webpack-dev-server": "^3.7.1"
  }
}
